<h1>Cuenta</h1>
<h1>Sus Datos son: <%- req.session.user.usuario %>  </h1>

<h2>Subir foto</h2>

<img src="images/<%- req.session.user.url%>" alt="<%- req.session.user.nombre %>" width="150" height="150">
<form action="/subirFoto" enctype="multipart/form-data" method="post">
    <input type="file" name="perfil">
    <input type="submit" value="Enviar">
    <div>
        <a href="/formCrearPokemon">Crear nuevo pokemon</a> 
    </div>
</form>
<div class="container">
    <ul>
        <% for(var i=0; i<cuentas.length; i++) {%>
        <% if(cuentas[i].id == req.session.user.id) {%>
            <dev>
                <li>
                    <label>Nombre: </label>
                    <%= cuentas[i].nombre %>
                        <label>Usuario: </label>
                        <%= cuentas[i].usuario %>
                </li>

                <ul>
                    <% for(var j=0; j<cuentas[i].misPokemon.length; j++) {%>
                        <li>
                            <%= cuentas[i].misPokemon[j].nombre %>
                                <img src="images/<%= cuentas[i].misPokemon[j].url %>" width="150" height="150">
                        </li>
                        <% } %>

                </ul>

            </dev>
        <% } %>
            <% } %>



    </ul>

</div>


<div class="container-fluid">

</div>
